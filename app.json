{
  "expo": {
    "name": "explorerWorld",
    "slug": "explorerWorld",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./src/images/EXPLORAR.INICIO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/images/EXPLORAR.INICIO.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "dependencies": {
      "expo": "~52.0.11",
      "react": "18.3.1",
      "react-native": "0.76.3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/images/EXPLORAR.INICIO.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/images/EXPLORAR.INICIO.png"
    },
    "scripts": {
      "postinstall": "npx jetify"
    },
    "plugins": ["expo-font"]
  }
}
